<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LandingActivity">

    <ImageView
        android:id="@+id/tr_blue_landing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tr_blue_tri_landing" />

    <ImageView
        android:id="@+id/tl_pink_landing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tl_pink_tri_landing" />

    <ImageView
        android:id="@+id/bl_blue_landing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/br_blue_tri_landing" />

    <ImageView
        android:id="@+id/bl_pink_landing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/bl_pink_tri_landing" />

    <ImageView
        android:id="@+id/fastfood_landing"
        android:layout_width="71dp"
        android:layout_height="67dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toStartOf="@+id/bakery_landing"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fastfood_landing" />

    <ImageView
        android:id="@+id/bakery_landing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        app:layout_constraintEnd_toEndOf="@+id/tl_pink_landing"
        app:layout_constraintStart_toStartOf="@+id/tl_pink_landing"
        app:layout_constraintTop_toTopOf="@+id/tl_pink_landing"
        app:srcCompat="@drawable/bakery_landing" />

    <ImageView
        android:id="@+id/pizza_landing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="180dp"
        app:layout_constraintEnd_toStartOf="@+id/bakery_landing"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pizza_landing" />

    <ImageView
        android:id="@+id/lunch_landing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="180dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/brunch_landing"
        app:srcCompat="@drawable/lunch_landing_logo" />

    <ImageView
        android:id="@+id/brunch_landing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/bl_blue_landing"
        app:srcCompat="@drawable/brunch_land" />

    <ImageView
        android:id="@+id/ramen_landing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/brunch_landing"
        app:srcCompat="@drawable/ramen_landing" />

    <Button
        android:id="@+id/im_hungry_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-35dp"
        android:text="I'm hungry!"
        app:backgroundTint="#6665DD"
        app:iconTint="#27233A"
        app:layout_constraintEnd_toEndOf="@+id/pic_logo_landing"
        app:layout_constraintStart_toStartOf="@+id/pic_logo_landing"
        app:layout_constraintTop_toBottomOf="@+id/pic_logo_landing"
        app:strokeColor="#27233A"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/pic_logo_landing"
        android:layout_width="276dp"
        android:layout_height="237dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pic_logo_clear" />

</androidx.constraintlayout.widget.ConstraintLayout>